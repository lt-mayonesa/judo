{
  "testPathIgnorePatterns": [
    "__tests__/helpers.js"
  ],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/cucumber/**/*.js"
  ],
  "setupFiles": [
    "./__tests__/helpers.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 60,
      "functions": 60,
      "lines": 60,
      "statements": 60
    }
  }
}
